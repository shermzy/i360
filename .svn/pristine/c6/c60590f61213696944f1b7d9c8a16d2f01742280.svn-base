		query += "inner join tblAssignment on tblAssignment.TargetLoginID = tblGap.TargetLoginID AND tblAssignment.SurveyID = tblGap.SurveyID "; //Added to fix unsynchronised result
		query = query + "WHERE tblGap.SurveyID = " + surveyID;
		query = query + " AND tblGap.CompetencyID = " + compID;
		
		if(divID != 0) 
			query = query + " AND tblAssignment.FKTargetDivision = " + divID;
