	/**
	 * 	Retrieves target ranking. Target with the minimum gap will be the top ranking
	 *	
	 *	@return ResultSet TargetRank
	 */
	public double getCompGap(int FKUser) throws SQLException 
	{
		String query = "SELECT CompetencyID, round(AVG(Gap), 2) AS Gap FROM tblGap WHERE SurveyID = " + surveyID;
		query += " AND TargetLoginID = " + FKUser + " GROUP BY CompetencyID ORDER BY CompetencyID";

		if(db.con == null) db.openDB();
		Statement stmt = db.con.createStatement();
		ResultSet rs = stmt.executeQuery(query);
		
		double gap = 0;
		
		while(rs.next())
			gap += rs.getDouble(2);	
		
		
		return gap;
	}
	
	
	/**
	 * 	Retrieves target ranking
	 *	@param int iOrder	0 = Highest gap to lowest gap (Desc) in Top Down direction
	 *						1 = Lowest gap to highest gap (Asc) in Top Down direction
	 *
	 *	@return ResultSet TargetRank
	 */
	public ResultSet getTargetRank(int iOrder) throws SQLException 
	{
		String query = "SELECT tblGap.TargetLoginID, SUM(tblGap.Gap) AS Total FROM tblGap ";
		
		query = query + "INNER JOIN [User] ON tblGap.TargetLoginID = [User].PKUser ";
		query += "inner join tblAssignment on tblAssignment.TargetLoginID = tblGap.TargetLoginID AND tblAssignment.SurveyID = tblGap.SurveyID "; //Added to fix unsynchronised result
		query = query + "WHERE tblAssignment.SurveyID = " + surveyID;
		
		if(divID != 0) 
			query = query + " AND tblAssignment.FKTargetDivision = " + divID;
			//query = query + " AND [User].FKDivision = " + divID;
		if(deptID != 0) 
			query = query + " AND tblAssignment.FKTargetDepartment = " + deptID;
			//query = query + " AND [User].FKDepartment = " + deptID;
		if(groupSection != 0)
			query = query + " AND tblAssignment.FKTargetGroup = " + groupSection;
			//query = query + " AND [User].Group_Section = " + groupSection;
		
		if(iOrder == 0)
			query = query + " GROUP BY tblGap.TargetLoginID ORDER BY SUM(tblGap.Gap) DESC";
		else
			query = query + " GROUP BY tblGap.TargetLoginID ORDER BY SUM(tblGap.Gap)";

		if(db.con == null) db.openDB();
		Statement stmt = db.con.createStatement();
		ResultSet rs = stmt.executeQuery(query);
		
		return rs;
	}
	
	
