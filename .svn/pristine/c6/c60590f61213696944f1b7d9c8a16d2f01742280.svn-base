		if(surveyLevel == 1)
			tableName = "tblResultBehaviour";
		
		if(totalCount == 1)
			formula = "sum(" + tableName + ".Result) AS Result ";
		else if(totalCount == 2)
			formula = "CAST(AVG(CAST(" + tableName + ".Result AS float)) AS numeric(38, 2)) AS Result ";
		else {
			if(reliabilityCheck == 0) {
				formula = "cast(cast((Sum(" + tableName + ".Result)-(Max(" + tableName + ".Result)+";
				formula = formula + "Min(" + tableName + ".Result))) as float)/(Count(" + tableName + ".Result)-2) as numeric(38,2)) AS Result ";
			}else
				formula = "CAST(AVG(CAST(" + tableName + ".Result AS float)) AS numeric(38, 2))  AS Result ";
		}
			
		if(surveyLevel == 0) 
