			// do sorting here
			for(int t=1; t<vLocal.size(); t++) {
				temp = Double.valueOf(((String [])vLocal.elementAt(t))[2]).doubleValue();
			
				if(type == 0) {
					if(temp > max) {
						max = temp;
						curr = t;
					}
				} else {
					if(temp < max) {
						max = temp;
						curr = t;
					}
				}
			}
			
			String info [] = {((String [])vLocal.elementAt(curr))[0], ((String [])vLocal.elementAt(curr))[1], ((String [])vLocal.elementAt(curr))[2]};
