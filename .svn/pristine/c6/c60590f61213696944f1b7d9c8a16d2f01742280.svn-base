	/**
	 * 	Calculate the average of gap for each competency
	 * 	This is only apply for KB Level Analysis
	 *	@param compID	CompetencyID
	 *
	 *	@return double AvgGap	Average gap for each competency
	 */
	public double getAvgGap(int compID) throws SQLException 
	{
		double gap = 0;

		String query = "SELECT tblGap.CompetencyID, CAST(AVG(CAST(tblGap.Gap AS float)) AS numeric(38, 2)) AS Result  ";
