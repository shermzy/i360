			double gap [] = new double [total];
			String target [] = new String [total];
			
			int i=0;
			
			//Modified by Jenty on 11/11/05
			//1. get all the completed raters.
			ResultSet rsRank = getAllTargets();	
			Vector vUser = new Vector();
			
			while(rsRank.next())			
				vUser.add(rsRank.getString(1));
	
			
