	public void InsertTargetRank() throws SQLException, IOException, Exception 
	{
		if (iNoCPR == 0)	// If CPR is chosen in this survey
		{
			System.out.println("8. Group Ranking Report");
		
			int maxScale = MaxScale();
			
			int [] address = OO.findString(xSpreadsheet, "<Group Rank>");
			
