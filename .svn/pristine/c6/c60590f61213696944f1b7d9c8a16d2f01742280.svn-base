		row = address[1];
		row++;
		
		String title = "Current Proficiency Vs Required Proficiency";
		
		int r = row+1;
		int c = 0;
		
		OO.insertString(xSpreadsheet3, "CP", r, c+1);
		if(type == 1)
			OO.insertString(xSpreadsheet3, "CPR", r, c+2);
		else {
			OO.insertString(xSpreadsheet3, "FPR", r, c+2);
			title = "Current Proficiency Vs Future Required Proficiency";
		}
		OO.insertString(xSpreadsheet3, "Prev. CP", r, c+3);
		r++;
		
		OO.findAndReplace(xSpreadsheet, "<Chart Title>", title);
		
		vCompID.clear();
		vGap.clear();
		
		int compID = 0;
		String compName = "";
		double dCP = 0;
		double dCPR = 0;
		double dPastCP = 0;
		
		for(int i=0; i<total; i++) 
		{								
			dCP = 0;
			dCPR = 0;
			dPastCP = 0;
			
			if(CP.next()) {
				compID = CP.getInt("CompetencyID");
				compName = UnicodeHelper.getUnicodeStringAmp(CP.getString("CompetencyName"));
				dCP = CP.getDouble("Result");					
			}
			else {
				compID = Integer.parseInt(((String [])vCompInfo.elementAt(i))[0]);
				compName = ((String [])vCompInfo.elementAt(i))[1];
				dCP = 0;
			}
			
			if(CPR.next())
				dCPR = CPR.getDouble("Result");
			else
				dCPR = 0;	
			
			if (PastCP != null && PastCP.next())
				dPastCP = PastCP.getDouble("Result");
			else
				dPastCP = 0;
			
			vCompID.add(new Integer(compID));
			double gap = Math.round((dCP - dCPR) * 100.0) / 100.0;;
			vGap.add(new String[]{compName, Double.toString(gap), Double.toString(dCP), Double.toString(dCPR)});
			
			OO.insertNumeric(xSpreadsheet3, dCP, r, c+1);
			OO.insertNumeric(xSpreadsheet3, dCPR, r, c+2);
			OO.insertNumeric(xSpreadsheet3, dPastCP, r, c+3);
			
			r++;
		}	
	}
	
	
