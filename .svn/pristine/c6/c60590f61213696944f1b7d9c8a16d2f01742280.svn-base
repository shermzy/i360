	 */
	public ResultSet Importance(int compID, int KBID) throws SQLException 
	{
		String query = "";
		int surveyLevel = Integer.parseInt(surveyInfo[0]);
		int reliabilityCheck = C. ReliabilityCheck(surveyID);
		
		String formula = "";
