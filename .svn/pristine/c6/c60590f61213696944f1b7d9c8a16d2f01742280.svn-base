	public void InsertCPvsCPR() throws SQLException, IOException, Exception 
	{
		System.out.println("4. Generating CP Versus CPR");
		String RTCode = "";
		int type = 1;
			
		ResultSet RT = RatingTask();
		
		ResultSet CP = null;
		ResultSet CPR = null;
		
		while(RT.next()) 
		{
			RTCode = RT.getString("RatingCode");
		
			if(RTCode.equals("CP") || RTCode.equals("CPR") || RTCode.equals("FPR")) 
			{
				if(RTCode.equals("CP"))
					CP = CPCPR(RTCode);
				else 
				{
					CPR = CPCPR(RTCode);
					if(RTCode.equals("CPR")) {
						type = 1;
						iNoCPR = 0;	// CPR is chosen in this survey
