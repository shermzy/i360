		column = address[0];
		row = address[1];
		
		OO.findAndReplace(xSpreadsheet, "<CP versus CPR Graph>", "");
		//OO.insertRows(xSpreadsheet, startColumn, endColumn, row, row+18, 18, 1);
		
		drawLineChart(CP, CPR, type);
	}
	
	
